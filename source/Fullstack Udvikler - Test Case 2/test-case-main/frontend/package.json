  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.46",
    "axios": "^1.7.4",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^10.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "react-query": "^3.39.2",
    "craco": "^0.0.3",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4",
    "usehooks-ts": "^2.9.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@craco/craco": "^7.1.0",
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@types/styled-components": "^5.1.24",
    "@types/react-redux": "^7.1.23",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.191",
    "@types/moment": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitlint": "^12.1.4",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^7.0.4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^3.2.2"
  },
  "scripts": {
    "postinstall": "command -v husky &>/dev/null && husky install || true",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --ext .ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint --fix",
      "prettier --write"
    ],
    "*.tsx": [
      "yarn lint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "run-if-changed": {
    "yarn.lock": "yarn install"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
